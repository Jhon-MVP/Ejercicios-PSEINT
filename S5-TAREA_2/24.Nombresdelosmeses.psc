Proceso Nombresdelosmeses
	Definir num Como Entero;
	
    Escribir "Ingrese un número del 1 al 12: ";
    Leer num;
    
    Segun num hacer
        Caso 1:
            Escribir "Enero";
        Caso 2:
            Escribir "Febrero";
        Caso 3:
            Escribir "Marzo";
        Caso 4:
            Escribir "Abril";
        Caso 5:
            Escribir "Mayo";
        Caso 6:
            Escribir "Junio";
        Caso 7:
            Escribir "Julio";
        Caso 8:
            Escribir "Agosto";
        Caso 9:
            Escribir "Septiembre";
        Caso 10:
            Escribir "Octubre";
        Caso 11:
            Escribir "Noviembre";
        Caso 12:
            Escribir "Diciembre";
        De Otro Modo:
            Escribir "Número no válido. Por favor ingrese un número del 1 al 12.";
    FinSegun
FinProceso
